    <script>
        d3.json('/api/RecupereCompagnie')
        .then(function(data) {
            // Dimensions du graphique
            const width = 400;
            const height = 400;
            const margin = { top: 20, right: 20, bottom: 30, left: 40 };
    
            // Créer l'échelle pour l'axe x
            const x = d3.scaleBand()
            .domain(data.map(d => d.compagnie))
            .range([margin.left, width - margin.right])
            .padding(0.1);
    
            // Créer l'échelle pour l'axe y
        
            const y = d3.scaleLinear()
            .domain([0, d3.max(data, d => parseInt(d.count))])
            .range([height - margin.bottom, margin.top]);
    
            // Créer l'élément SVG pour le graphique
            const svg = d3.select('#chart')
            .append('svg')
            .attr('width', width)
            .attr('height', height);
    
            // Créer les barres du graphique à barres
            svg.selectAll('rect')
            .data(data)
            .enter()
            .append('rect')
            .attr('x', d => x(d.compagnie))
            .attr('y', d => y(+d.count))
            .attr('width', x.bandwidth())
            .attr('height', d => height - margin.bottom - y(+d.count))
            .attr('fill', 'red')
            .each((d, i) => {
                console.log(d); // Afficher la valeur de d pour chaque élément
                })
    
            // Ajouter l'axe x
            const xAxis = d3.axisBottom(x);
    
            svg.append('g')
            .attr('transform', `translate(0, ${height - margin.bottom})`)
            .call(xAxis);
    
            // Ajouter l'axe y
            const yAxis = d3.axisLeft(y);
    
            svg.append('g')
            .attr('transform', `translate(${margin.left}, 0)`)
            .call(yAxis);
        })
        .catch(function(error) {
            console.error(error);
            // Gérer les erreurs, si nécessaire
        });
        d3.json('/api/recupereCategoriModele')
        .then(function(data){
          const typecouleur=['#FF0000', '#00FF00', '#0000FF', '#FFFF00', '#FF00FF', '#00FFFF', '#800000', '#008000',
          '#000080', '#808000', '#800080', '#008080', '#C0C0C0', '#808080', '#FFA500', '#FFC0CB',
          '#FFFFE0', '#F0E68C', '#ADD8E6', '#90EE90', '#D3D3D3', '#F0F8FF', '#FAEBD7', '#FFEFD5',
          '#FFE4B5', '#FFE4C4', '#F0FFFF', '#F5F5DC', '#F5FFFA', '#F8F8FF', '#FDF5E6', '#FFDEAD',
          '#FFDAB9', '#FFF0F5'];
  
          var valeur=[]
          var modele=[]
          data.forEach(Element=>{
              valeur.push(Element.count)
              modele.push(Element.modele)
          })
          
          var svg=d3.select("#chart2")
          .append('svg')
          .attr('width',500)
          .attr('height',800);
  
          var tabpie=d3.pie()
            .value(function(d){
              return d;
            })(valeur);
  
          var arc=d3.arc()
            .outerRadius(180)
            .innerRadius(0);
  
          var grp=svg.append("g")
            .attr("transform","translate(200,200)");
  
          var graph=grp.selectAll('path')
            .data(tabpie)
            .enter()
            .append("path")
            .attr("fill",function(d,i){
              return typecouleur[i];
            })
            .attr("d",arc);
            var leg = svg.selectAll("g.legende").data(modele);

            // Créer les groupes "g" pour chaque entrée de légende
            var legEnter = leg.enter()
              .append("g")
              .attr("class", "legende")
              .attr("transform", function(d, i) {
                return "translate(400," + (10 + 20 * i) + ")";
              });
            
            // Ajouter un rectangle et un texte à chaque groupe "g"
            legEnter.append("rect")
              .attr("width", 15)
              .attr("height", 15)
              .attr("fill", function(d, i) {
                return typecouleur[i];
              })
              .style("display", "inline-block"); ;
            
            legEnter.append("text")
              .attr("x", 25)
              .attr('y', 12)
              .attr("fill", "red")
              .style("font-size", "12px")
              .text(function(d) {
                console.log(d)
                return d;
              });
         
  
        });
    </script>
</body>

</html>